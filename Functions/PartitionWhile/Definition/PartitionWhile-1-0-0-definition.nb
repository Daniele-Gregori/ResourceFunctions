      "link" -> {{
        "title" -> "doi", "href" -> 
         "http://dx.doi.org/10.1145/3705328.3748031", "rel" -> "related"}, {
        "href" -> "http://arxiv.org/abs/2508.21334v1", "rel" -> "alternate", 
