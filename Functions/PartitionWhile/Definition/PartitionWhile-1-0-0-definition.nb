      "link" -> {{
        "title" -> "doi", "href" -> 
         "http://dx.doi.org/10.1002/adma.202500556", "rel" -> "related"}, {
        "href" -> "http://arxiv.org/abs/2508.21395v1", "rel" -> "alternate", 
