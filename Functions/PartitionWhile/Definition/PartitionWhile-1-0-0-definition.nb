Cell[BoxData[
 RowBox[{
  RowBox[{"pwlTot", "[", "list_", "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"fun", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Total", "[", "#", "]"}], "<=", "1000"}], "&"}]}], "}"}], ",",
     "\[IndentingNewLine]", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "len", ",", "i", ",", "j", ",", "first", ",", "last", ",", "elq", ",", 
        "prtind"}], "}"}], ",", "\[IndentingNewLine]", "\t\t\t", 
      "\[IndentingNewLine]", "\t\t\t", 
      RowBox[{
       RowBox[{"len", "=", 
        RowBox[{"Length", "[", "list", "]"}]}], ";", "\[IndentingNewLine]", 
       "\t\t\t", "\[IndentingNewLine]", "\t\t\t", 
       RowBox[{"prtind", "=", 
        RowBox[{"{", "}"}]}], ";", "\t\t\t", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", "\t\t\t", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "<=", "len"}], ",", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", "\t\t\t\t", 
